{
  "name": "rosi",
  "version": "1.1.4",
  "description": "Electron GUI for yt-dlp",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "electron": "electron .",
    "build": "electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BurntToasters/rosi.git"
  },
  "author": "BurntToasters <code@rosie.run>",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/BurntToasters/rosi/issues"
  },
  "homepage": "https://github.com/BurntToasters/rosi#readme",
  "devDependencies": {
    "electron": "^35.5.1",
    "electron-builder": "^26.0.12",
    "electron-packager": "^17.1.2"
  },
  "dependencies": {
    "glob": "^11.0.1"
  },
  "build": {
    "appId": "BurntToasters.ROSI",
    "productName": "ROSI",
    "files": [
      "**/*",
      "!**/node_modules/*/{test,__tests__,tests}/**",
      "!**/*.md",
      "!**/package-devonly.js",
      "!**/dist"
    ],
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": [
        "nsis",
        "appx"
      ],
      "icon": "build/app-icon.png",
      "asarUnpack": [
        "yt-dlp.exe"
      ]
    },
    "appx": {
      "identityName": "BurntToasters.ROSI",
      "publisher": "CN=BC0CB990-55E8-4580-97B7-1E2DAA4B44F6",
      "publisherDisplayName": "BurntToasters",
      "displayName": "ROSI",
      "backgroundColor": "#ffffff",
      "languages": [
        "en-US"
      ],
      "tileImages": {
        "square44x44Logo": "build/Square44x44Logo.png",
        "square71x71Logo": "build/Square71x71Logo.png",
        "square150x150Logo": "build/Square150x150Logo.png",
        "wide310x150Logo": "build/Wide310x150Logo.png",
        "square310x310Logo": "build/Square310x310Logo.png",
        "storeLogo": "build/StoreLogo.png"
      }
    },
    "mac": {
      "target": "dmg",
      "icon": "build/app-icon.png",
      "asarUnpack": [
        "yt-dlp_macos"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Utility",
      "asarUnpack": [
        "yt-dlp_linux"
      ]
    },
    "dmg": {
      "background": "build/dmg-background.png",
      "icon": "build/app-icon.png",
      "iconSize": 100,
      "contents": [
        {
          "x": 120,
          "y": 300,
          "type": "file"
        },
        {
          "x": 546,
          "y": 300,
          "type": "link",
          "path": "/Applications"
        }
      ],
      "window": {
        "width": 648,
        "height": 480
      }
    }
  }
}